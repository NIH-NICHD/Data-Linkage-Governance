    <!-- https://www.nichd.nih.gov/data_governance_odrl/Link -->

    <owl:NamedIndividual rdf:about="https://www.nichd.nih.gov/data_governance_odrl/Link">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdf:type rdf:resource="http://www.w3.org/ns/odrl/2/Action"/>
        <odrl:implies rdf:resource="http://www.w3.org/ns/odrl/2/aggregate"/>
        <odrl:includedIn rdf:resource="http://www.w3.org/ns/odrl/2/use"/>
        <rdfs:label>Link</rdfs:label>
        <skos:definition>A study in which data from different sources are &quot;linked&quot;. Usually used to compile epidemiological data. The logic of record linkage is that two or more items of information about a person recorded at different times, and perhaps in different places, may be of greater value when considered together than when either is considered alone.

[source: https://thesaurus.cancer.gov/ncitbrowser/ConceptReport.jsp?dictionary=NCI_Thesaurus&amp;ns=ncit&amp;code=C15424]</skos:definition>
        <skos:exactMatch>https://thesaurus.cancer.gov/ncitbrowser/ConceptReport.jsp?dictionary=NCI_Thesaurus&amp;ns=ncit&amp;code=C15424</skos:exactMatch>
    </owl:NamedIndividual>
    


