     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:odrl="http://www.w3.org/ns/odrl/2/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:data_governance_odrl="https://github.com/NIH-NICHD/Data-Linkage-Governance/">
    <owl:Ontology rdf:about="https://github.com/NIH-NICHD/Data-Linkage-Governance">
        <owl:imports rdf:resource="https://www.w3.org/ns/odrl/2/ODRL22.rdf"/>
        <terms:license>Apache License, Version 2.0</terms:license>
        <rdfs:comment>Copyright 2024 The MITRE Corporation</rdfs:comment>
        <rdfs:comment xml:lang="en">This ontology provides a profile for the ODRL 2.2 specification that is specific to data governance and management in the context of data linkage.</rdfs:comment>
    </owl:Ontology>
    


